{
    "layout": {
        "type": "grid",
        "space": "2mm",
        "rows": 8,
        "cols": 10,
        "hbackbone": "4mm",
        "vbackbone": "4mm",
        "hboneskip": "1",
        "vboneskip": "1",
        "hbonecut": false,
        "vbonecut": false
    },
    "tabs": {
        "type": "fixed",
        "hcount": 1,
        "hwidth": "2mm",
        "vcount": 1,
        "vwidth": "2mm"
    },
    "cuts": {
        "type": "mousebites",
        "drill": "0.5mm",
        "spacing": "1mm",
        "offset": "0mm"
    },
    "framing": {
        "type": "frame",
        "width": "6mm",
        "space": "2mm",
        "mintotalheight": "70mm",
        "mintotalwidth": "70mm"
    },
    "tooling": {
        "type": "3hole",
        "hoffset": "3mm",
        "voffset": "3mm",
        "size": "1.5mm"
    },
    "fiducials": {
        "type": "3fid",
        "hoffset": "6.5mm",
        "voffset": "3.75mm",
        "coppersize": "2mm",
        "opening": "1mm"
    },
    "post": {
        "millradius": "0mm"
    },
    "text": {
        "type": "simple",
        "text": "{boardTitle} {boardRevision} {date}",
        "anchor": "mt",
        "voffset": "2.5mm",
        "hjustify": "center",
        "vjustify": "center"
    },
    "text2": {
        "type": "simple",
        "text": "JLCJLCJLCJLC",
        "anchor": "mb",
        "voffset": "-2.5mm",
        "hjustify": "center",
        "vjustify": "center"
    }
}
