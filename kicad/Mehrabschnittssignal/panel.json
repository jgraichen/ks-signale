{
    "layout": {
        "type": "grid",
        "space": "2mm",
        "rows": 4,
        "cols": 7
    },
    "tabs": {
        "type": "fixed",
        "hcount": 1,
        "hwidth": "5mm",
        "vcount": 1,
        "vwidth": "3mm"
    },
    "cuts": {
        "type": "vcuts",
        "layer": "Edge.Cuts"
    },
    "framing": {
        "type": "frame",
        "width": "6mm",
        "space": "2mm",
        "mintotalheight": "70mm",
        "mintotalwidth": "70mm"
    },
    "tooling": {
        "type": "3hole",
        "hoffset": "3mm",
        "voffset": "3mm",
        "size": "1.5mm"
    },
    "fiducials": {
        "type": "3fid",
        "hoffset": "6.5mm",
        "voffset": "3.75mm",
        "coppersize": "2mm",
        "opening": "1mm"
    },
    "post": {
        "millradius": "1mm"
    },
    "text": {
        "type": "simple",
        "text": "{boardTitle} {boardRevision} {date}",
        "anchor": "mt",
        "voffset": "2.5mm",
        "hjustify": "center",
        "vjustify": "center"
    }
}
